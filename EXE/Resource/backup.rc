    POPUP "オプション(&O)"
    BEGIN
        MENUITEM "環境設定(&S)...",                 MENU_SETENV
    END
    POPUP "ログ(&L)"
    BEGIN
        MENUITEM "ログファイルを表示(&S)",               MENU_DISPLOG
