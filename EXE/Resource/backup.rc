            MENUITEM "設定をファイルに保存(&S)...",           MENU_REGSAVE
            MENUITEM "設定をファイルから復元(&L)...",          MENU_REGLOAD
            MENUITEM SEPARATOR
            MENUITEM "全設定の消去...",                   MENU_REGINIT
        END
        MENUITEM SEPARATOR
        MENUITEM "終了(&X)",                      MENU_FILE_EXIT
