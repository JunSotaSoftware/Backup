main_menu MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "Start Selected &Patterns...", MENU_BACKUP
        MENUITEM "&Quick Backup...",            MENU_QUICK_BACKUP
        MENUITEM SEPARATOR
        POPUP "&Settings"
        BEGIN
            MENUITEM "&Save Settings to File...",   MENU_REGSAVE
            MENUITEM "&Load Settings from File...", MENU_REGLOAD
            MENUITEM SEPARATOR
            MENUITEM "&Clear All Settings...",      MENU_REGINIT
        END
        MENUITEM SEPARATOR
        MENUITEM "E&xit",                       MENU_FILE_EXIT
    END
    POPUP "&Options"
    BEGIN
        MENUITEM "&Settings",                   MENU_SETENV
    END
    POPUP "&Log"
    BEGIN
        MENUITEM "&Show Logfile",               MENU_DISPLOG
        MENUITEM "Show &error log on last backup", MENU_ERROR_LOG
        MENUITEM "&Open Logfile folder",        MENU_LOGFOLDER
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&Contents",                   MENU_HELP_CONTENT
        MENUITEM SEPARATOR
        MENUITEM "&About Backup...",            MENU_HELP_ABOUT
    END
END

